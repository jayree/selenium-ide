{
  "name": "selenium-side-runner",
  "version": "3.3.1",
  "description": "Run Selenium IDE projects in cli",
  "repository": "https://github.com/SeleniumHQ/selenium-ide",
  "scripts": {
    "build": "babel --out-dir dist src --ignore=__test__ --verbose",
    "pkg": "pkg package.json --targets=node9-linux-x64,node9-macos-x64,node9-windows-x64 --out-path=dist/bin",
    "prepublishOnly": "yarn build"
  },
  "bin": {
    "selenium-side-runner": "dist/index.js"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "selenium",
    "command",
    "line"
  ],
  "license": "Apache-2.0",
  "private": false,
  "pkg": {
    "scripts": [
      "node_modules/jest-*/**/*.js",
      "node_modules/node-gyp/**/*.js"
    ]
  },
  "dependencies": {
    "commander": "^2.18.0",
    "glob": "^7.1.3",
    "global-npm": "^0.3.0",
    "jest": "23.6.0",
    "jest-environment-selenium": "^1.1.0",
    "js-beautify": "^1.8.6",
    "js-yaml": "^3.12.0",
    "rimraf": "^2.6.2",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "selianize": "^3.3.1",
    "semver": "^5.5.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.0",
    "@babel/core": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "npm": "^6.4.1",
    "pkg": "https://github.com/corevo/pkg#a5a5e8717ca7ce17f51c32b4f82883f5cea9839a"
  }
}
